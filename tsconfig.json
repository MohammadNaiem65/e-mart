{
  "compilerOptions": {
    "target": "es2016",  
    // "lib": [],       
    "module": "commonjs",
    "rootDir": "./", 
    "outDir": "./dist",  
    "baseUrl": "./",  
    "paths": {
			"@/*": ["./*"]
		}, 
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true, 
    "allowSyntheticDefaultImports": true, 
    "noImplicitAny": true,
    "strictNullChecks": true, 
  },
}

